/*===================== GRADIENT ======================*/
@mixin linear-gradient ($from-color,$to-color) {
	background:$from-color; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  $from-color 0%, $to-color 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,$from-color), color-stop(100%,$to-color)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  $from-color 0%,$to-color 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  $from-color 0%,$to-color 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  $from-color 0%,$to-color 100%); /* IE10+ */
	background: linear-gradient(to bottom,  $from-color 0%,$to-color 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='$from-color', endColorstr='$to-color',GradientType=0 ); /* IE6-8 */
}
@mixin linear-gradient-three-colors ($first-color,$second-color,$third-color) {
	background: $first-color; /* Old browsers */
	background: -moz-linear-gradient(top,  $first-color 0%, $second-color 10%, $third-color 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,$first-color), color-stop(10%,$second-color), color-stop(100%,$third-color)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  $first-color 0%,$second-color 10%,$third-color 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  $first-color 0%,$second-color 10%,$third-color 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  $first-color 0%,$second-color 10%,$third-color 100%); /* IE10+ */
	background: linear-gradient(to bottom,  $first-color 0%,$second-color 10%,$third-color 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='$first-color', endColorstr='$third-color',GradientType=0 ); /* IE6-9 */
}
